<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.myapp.remover.pro3.MainActivity"
    android:background="#eb3f06">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/n1"
        android:background="#e28989"
        android:textSize="45dp"
        android:textIsSelectable="false"
        android:textColor="#15dac7"
        android:password="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"

        android:layout_height="62dp"

        android:text="1"
        android:id="@+id/num1"
        android:textSize="34dp"
        android:layout_below="@+id/n1"
        android:layout_alignLeft="@+id/n1"
        android:layout_alignStart="@+id/n1" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="2"
        android:id="@+id/num2"
        android:textSize="34dp"
        android:layout_toRightOf="@+id/num1"
        android:layout_toEndOf="@+id/num1"
        android:layout_above="@+id/num4"
        android:layout_alignTop="@+id/num1" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="4"
        android:id="@+id/num4"
        android:textSize="34dp"
        android:layout_below="@+id/num1"
        android:layout_toLeftOf="@+id/num2"
        android:layout_toStartOf="@+id/num2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="5"
        android:id="@+id/num5"
        android:textSize="34dp"
        android:layout_alignBaseline="@+id/num0"
        android:layout_alignBottom="@+id/num0" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="6"
        android:id="@+id/num6"
        android:textSize="34dp"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/num9"
        android:layout_toStartOf="@+id/num9" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="7"
        android:id="@+id/num7"
        android:textSize="34dp"
        android:layout_below="@+id/num4"
        android:layout_alignLeft="@+id/num4"
        android:layout_alignStart="@+id/num4" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="8"
        android:id="@+id/num8"
        android:textSize="34dp"
        android:layout_below="@+id/add"
        android:layout_toRightOf="@+id/num6"
        android:layout_toEndOf="@+id/num6" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="9"
        android:id="@+id/num9"
        android:textSize="34dp"
        android:layout_below="@+id/num4"
        android:layout_toRightOf="@+id/num2"
        android:layout_toEndOf="@+id/num2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="C"
        android:id="@+id/clr"
        android:textSize="34dp"
        android:layout_below="@+id/mul"
        android:layout_alignLeft="@+id/num7"
        android:layout_alignStart="@+id/num7" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="0"
        android:id="@+id/num0"
        android:textSize="34dp"
        android:layout_below="@+id/num7"
        android:layout_toLeftOf="@+id/num2"
        android:layout_toStartOf="@+id/num2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="62dp"
        android:text="="
        android:id="@+id/eq"
        android:textSize="34dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="50dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="/"
        android:id="@+id/div"
        android:textSize="34dp"
        android:layout_alignTop="@+id/clr"
        android:layout_alignLeft="@+id/mul"
        android:layout_alignStart="@+id/mul" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="*"
        android:id="@+id/mul"
        android:textSize="34dp"
        android:layout_alignBaseline="@+id/num9"
        android:layout_alignBottom="@+id/num9"
        android:layout_alignLeft="@+id/sub"
        android:layout_alignStart="@+id/sub" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="-"
        android:id="@+id/sub"
        android:textSize="34dp"
        android:layout_below="@+id/num10"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="+"
        android:id="@+id/add"
        android:textSize="34dp"
        android:layout_below="@+id/n1"
        android:layout_alignRight="@+id/n1"
        android:layout_alignEnd="@+id/n1" />

    <Button
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="."
        android:id="@+id/num10"
        android:textSize="34dp"
        android:layout_below="@+id/num9"
        android:layout_alignLeft="@+id/num9"
        android:layout_alignStart="@+id/num9" />

    <RelativeLayout tools:context="com.myapp.remover.pro3.MainActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:background="#eb3f06"
        android:layout_above="@+id/num8"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="1"
            android:id="@+id/button"
            android:textSize="34dp"
            android:layout_below="@+id/editText"
            android:layout_alignLeft="@+id/editText"
            android:layout_alignStart="@+id/editText" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="4"
            android:id="@+id/button3"
            android:textSize="34dp"
            android:layout_below="@+id/button"
            android:layout_toLeftOf="@+id/button2"
            android:layout_toStartOf="@+id/button2" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="5"
            android:id="@+id/button4"
            android:textSize="34dp"
            android:layout_alignBaseline="@+id/button10"
            android:layout_alignBottom="@+id/button10" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="7"
            android:id="@+id/button6"
            android:textSize="34dp"
            android:layout_below="@+id/button3"
            android:layout_alignLeft="@+id/button3"
            android:layout_alignStart="@+id/button3" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="8"
            android:id="@+id/button7"
            android:textSize="34dp"
            android:layout_below="@+id/button15"
            android:layout_toRightOf="@+id/button5"
            android:layout_toEndOf="@+id/button5" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="9"
            android:id="@+id/button8"
            android:textSize="34dp"
            android:layout_below="@+id/button3"
            android:layout_toRightOf="@+id/button2"
            android:layout_toEndOf="@+id/button2" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="C"
            android:id="@+id/button9"
            android:textSize="34dp"
            android:layout_below="@+id/button13"
            android:layout_alignLeft="@+id/button6"
            android:layout_alignStart="@+id/button6" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="0"
            android:id="@+id/button10"
            android:textSize="34dp"
            android:layout_below="@+id/button6"
            android:layout_toLeftOf="@+id/button2"
            android:layout_toStartOf="@+id/button2" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="fill_parent"
            android:layout_height="62dp"
            android:text="="
            android:id="@+id/button11"
            android:textSize="34dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="50dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="/"
            android:id="@+id/button12"
            android:textSize="34dp"
            android:layout_alignTop="@+id/button9"
            android:layout_alignLeft="@+id/button13"
            android:layout_alignStart="@+id/button13" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="*"
            android:id="@+id/button13"
            android:textSize="34dp"
            android:layout_alignBaseline="@+id/button8"
            android:layout_alignBottom="@+id/button8"
            android:layout_alignLeft="@+id/button14"
            android:layout_alignStart="@+id/button14" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="-"
            android:id="@+id/button14"
            android:textSize="34dp"
            android:layout_below="@+id/button16"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="+"
            android:id="@+id/button15"
            android:textSize="34dp"
            android:layout_below="@+id/editText"
            android:layout_alignRight="@+id/editText"
            android:layout_alignEnd="@+id/editText" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText"
            android:background="#e28989"
            android:textSize="45dp"
            android:textIsSelectable="false"
            android:textColor="#15dac7"
            android:password="false"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:text="."
            android:id="@+id/button16"
            android:textSize="34dp"
            android:layout_below="@+id/button8"
            android:layout_alignLeft="@+id/button8"
            android:layout_alignStart="@+id/button8" />
    </RelativeLayout>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:text="6"
        android:id="@+id/button5"
        android:textSize="34dp"
        android:layout_alignBaseline="@+id/sub"
        android:layout_alignBottom="@+id/sub"
        android:layout_toRightOf="@+id/num9"
        android:layout_toEndOf="@+id/num9" />


</RelativeLayout>
